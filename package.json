{
  "name": "rkta",
  "version": "0.0.6",
  "scripts": {
    "bootstrap": "lerna clean && npm run clean:tsbuildinfo && lerna bootstrap",
    "build:babel": "lerna run build",
    "prebuild": "npm run clean:tsbuildinfo",
    "build": "tsc --build",
    "bump": "npm run make && lerna version patch",
    "clean:tsbuildinfo": "rm -rf src/packages/*/tsconfig.tsbuildinfo",
    "clean:docz-cache": "rm -rf .docz/.cache",
    "predocz:dev": "npm run clean:docz-cache",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "lerna run lint",
    "make": "npm run lint && npm run build",
    "publish": "npm run make && lerna publish --contents lib --yes patch",
    "updatedeps": "ncu -u && npm update",
    "typecheck": "lerna run typecheck"
  },
  "engines": {
    "yarn": "^1.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-emotion": "^10.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "lerna": "^3.19.0",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "react-dom": "^16.9.0",
    "typescript": "^3.7.3"
  },
  "licence": "MIT",
  "private": true,
  "resolutions": {
    "@types/react": "^16.9.15"
  },
  "workspaces": [
    "src/packages/*"
  ]
}