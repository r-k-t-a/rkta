{
  "name": "rkta",
  "version": "0.0.6",
  "scripts": {
    "bootstrap": "lerna clean && yarn run clean:tsbuildinfo && lerna bootstrap",
    "prebuild": "yarn run clean:tsbuildinfo && lerna run prebuild",
    "build": "tsc --build && lerna run build:esm",
    "bump": "yarn run make && lerna version patch",
    "clean:tsbuildinfo": "rm -rf src/packages/*/tsconfig.tsbuildinfo",
    "clean:docz-cache": "rm -rf .docz/.cache",
    "clean:lib": "rm -rf src/packages/*/lib",
    "predocz:dev": "yarn run clean:docz-cache",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "lerna run lint",
    "make": "yarn run lint && yarn run build",
    "release": "yarn run make && lerna publish --contents lib --yes patch",
    "updatedeps": "ncu -u && yarn",
    "typecheck": "lerna run typecheck"
  },
  "engines": {
    "yarn": "^1.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "babel-plugin-emotion": "^10.0.33",
    "cz-conventional-changelog": "^3.2.0",
    "docz": "^2.3.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "lerna": "^3.22.0",
    "npm-check-updates": "^6.0.1",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "typescript": "3.9.3"
  },
  "licence": "MIT",
  "private": true,
  "resolutions": {
    "@types/react": "^16.9.35"
  },
  "workspaces": [
    "src/packages/*"
  ]
}
