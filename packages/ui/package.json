{
  "name": "@rkta/ui",
  "version": "0.0.13",
  "description": "WIP Lightweight UI library for react",
  "author": "Sergei Droganov <serge@droganov.ru>",
  "homepage": "https://github.com/r-k-t-a/rkta",
  "license": "MIT",
  "main": "src/index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-k-t-a/rkta.git"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --ignore \"**/*.spec.ts\" --source-maps",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx src",
    "prepare": "cp package.json lib/package.json || exit 0",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/r-k-t-a/rkta/issues"
  },
  "licence": "MIT",
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "@types/lodash": "^4.14.137",
    "@types/react": "^16.9.2",
    "emotion-theming": "^10.0.14",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2"
  },
  "gitHead": "28b39e9d4f56c5a36c6766fc9302d72dc30c3f3a"
}
