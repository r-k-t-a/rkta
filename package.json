{
  "name": "rkta",
  "version": "0.0.6",
  "scripts": {
    "bootstrap": "lerna clean && npm run clean:tsbuildinfo && lerna bootstrap",
    "build:babel": "lerna run build",
    "prebuild": "npm run clean:tsbuildinfo",
    "build": "tsc --build",
    "postbuild": "npm run mjs",
    "bump": "npm run make && lerna version patch",
    "clean:tsbuildinfo": "rm -rf src/packages/*/tsconfig.tsbuildinfo",
    "clean:docz-cache": "rm -rf .docz/.cache",
    "clean:lib": "rm -rf src/packages/*/lib",
    "mjs": "renamer --force --find js --replace mjs 'src/packages/**/lib/**'",
    "predocz:dev": "npm run clean:docz-cache",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "lerna run lint",
    "make": "npm run lint && npm run build",
    "publish": "npm run make && lerna publish --contents lib --yes patch",
    "updatedeps": "ncu -u && npm update",
    "typecheck": "lerna run typecheck"
  },
  "engines": {
    "yarn": "^1.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-plugin-emotion": "^10.0.27",
    "cz-conventional-changelog": "^3.1.0",
    "docz": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "lerna": "^3.20.2",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "react-dom": "^16.12.0",
    "renamer": "^1.1.4",
    "typescript": "^3.8.2"
  },
  "licence": "MIT",
  "private": true,
  "resolutions": {
    "@types/react": "^16.9.23"
  },
  "workspaces": [
    "src/packages/*"
  ]
}
