{
  "name": "rkta",
  "version": "0.0.6",
  "scripts": {
    "bootstrap": "lerna clean && lerna bootstrap",
    "build": "lerna run build",
    "build:tsc": "tsc --build",
    "bump": "npm run make && lerna version patch",
    "commit": "git-cz",
    "predocz:dev": "rm -rf .docz/cache",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "lerna run lint",
    "make": "npm run lint && npm run build",
    "postinstall": "lerna bootstrap",
    "publish": "npm run make && lerna publish --contents lib --yes patch",
    "updatedeps": "ncu -u && npm update",
    "typecheck": "lerna run typecheck"
  },
  "engines": {
    "yarn": "^1.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.11",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-plugin-emotion": "^10.0.22",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^1.3.2",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "lerna": "^3.18.3",
    "npm-check-updates": "^3.1.25",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  },
  "licence": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
